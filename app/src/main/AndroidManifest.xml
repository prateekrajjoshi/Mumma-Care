<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.user.mumma_care" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="22" />

    <uses-permission android:name="android.permission.SEND_SMS" />

    <android:uses-permission
        android:name="android.permission.WRITE_EXTERNAL_STORAGE"
        android:maxSdkVersion="18" />
    <android:uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <android:uses-permission
        android:name="android.permission.READ_EXTERNAL_STORAGE"
        android:maxSdkVersion="18" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity android:name=".MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".NewPatientScreen"
            android:label="@string/title_activity_new_patient_screen" >
        </activity>
        <activity
            android:name=".ExistingPatientScreen"
            android:label="@string/title_activity_existing_patient_screen" >
        </activity>
        <activity
            android:name=".ExistingUserInformation"
            android:label="@string/title_activity_existing_user_information" >
        </activity>
        <activity
            android:name=".SendSMS"
            android:label="@string/title_activity_send_sms" >
        </activity>
        <activity
            android:name=".FirstScreen"
            android:label="@string/title_activity_first_screen" >
        </activity>
        <activity
            android:name=".SavedNumbers"
            android:label="@string/title_activity_saved_numbers" >
        </activity>
        <activity
            android:name=".First_Visit"
            android:label="@string/title_activity_first__visit" >
        </activity>
        <activity
            android:name=".Second_Visit"
            android:label="@string/title_activity_second__visit" >
        </activity>
        <activity
            android:name=".Third_Visit"
            android:label="@string/title_activity_third__visit" >
        </activity>
        <activity
            android:name=".Fourth_Visit"
            android:label="@string/title_activity_fourth__visit" >
        </activity>
        <activity
            android:name=".Fifth_Visit"
            android:label="@string/title_activity_fifth__visit" >
        </activity>
    </application>

</manifest>
